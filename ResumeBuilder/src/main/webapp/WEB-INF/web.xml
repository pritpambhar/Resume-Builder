<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AddData</servlet-name>
  	<display-name>AddData</display-name>
  	<description></description>
  	<servlet-class>com.rb.controller.AddData</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetData</servlet-name>
  	<display-name>GetData</display-name>
  	<description></description>
  	<servlet-class>com.rb.controller.GetData</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GeneralInfo</servlet-name>
  	<display-name>GeneralInfo</display-name>
  	<description></description>
  	<servlet-class>com.rb.controller.GeneralInfo</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PublicProfile</servlet-name>
  	<display-name>PublicProfile</display-name>
  	<description></description>
  	<servlet-class>com.rb.controller.PublicProfile</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>AddData</servlet-name>
  	<url-pattern>/addData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetData</servlet-name>
  	<url-pattern>/getData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GeneralInfo</servlet-name>
  	<url-pattern>/generalInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PublicProfile</servlet-name>
  	<url-pattern>/publicProfile</url-pattern>
  </servlet-mapping>
  
</web-app>
